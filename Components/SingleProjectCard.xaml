<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MVPStudio_Creative_Agency.Components.SingleProjectCard">
    <VerticalStackLayout>
        <Frame CornerRadius="20" BackgroundColor="{DynamicResource WhiteDark}" BorderColor="Transparent" >
            <HorizontalStackLayout>
                <Label FontSize="16" FontFamily="Hind-Regular" TextColor="{DynamicResource Black}"/>
            </HorizontalStackLayout>
        </Frame>
    </VerticalStackLayout>
</ContentView>

